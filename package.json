{
  "name": "shipping_routes_app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/shipping_routes_app/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/shipping_routes_app/test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "auth_migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./apps/auth_service/src/migrations/add-address-column-to-user-table -d ./apps/auth_service/src/datasources/migration.ts",
    "auth_migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./apps/auth_service/src/datasources/migration.ts",
    "route_migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./apps/route_service/src/migrations/add-address-column-to-user-table -d ./apps/route_service/src/datasources/migration.ts",
    "route_migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./apps/route_service/src/datasources/migration.ts",
    "auth_seed:run": "npx ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./apps/auth_service/src/datasources/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.676.0",
